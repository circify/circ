struct Bar {
    u8 d
    u16 e
}

struct Foo {
    u32[7] a
    field b
    u64 c
    Bar d
}

def main() -> Foo:
    Bar w = Bar { d: 0, e: 0 }
    assert(w == w)

    Foo x = Foo { a: [7; 7], b: 1, c: 0, d: w }
    Foo y = Foo { a: [8; 7], b: 0, c: 1, d: w }

    assert(x != y)
    assert(!(x == y))
    return x
