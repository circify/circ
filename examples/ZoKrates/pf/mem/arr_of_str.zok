const u32 LEN = 6
const u32 ACCESSES = 3

struct Pt {
    field x
    field y
    field z
}
const transcript Pt [LEN] array = [Pt {x: 4, y: 5, z: 6}, ...[Pt {x: 0, y: 1, z: 2}; LEN - 1]]

def main(private field[ACCESSES] idx) -> field:
    field prod = 1
    for u32 i in 0..ACCESSES do
        field access = idx[i]
        Pt pt = array[access]
        prod = prod * pt.x * pt.y * pt.z
    endfor
    return prod
